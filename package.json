{
  "type": "module",
  "packageManager": "pnpm@9.1.4",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "name": "athena",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "pnpm install --force tailwindcss postcss autoprefixer",
    "dev": "next dev",
    "dev:all": "powershell -ExecutionPolicy Bypass -File start-dev.ps1",
    "build": "next build",
    "postinstall": "pnpm install tailwindcss --force && prisma generate",
    "build:clean": "if exist .next rmdir /s /q .next && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "tailwindcss": "^3.4.17",
